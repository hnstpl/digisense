        fd.append("VideoDescription", $("#tractorConfiguration_dIYVideo_VideoDescription").val());
        fd.append("SelectedVideoCategory", $("#CategoryList").val());
        fd.append("VideoFile", $("#tractorConfiguration_dIYVideo_VideoFile").get(0).files[0]);

        $.ajax({
            async: false,
