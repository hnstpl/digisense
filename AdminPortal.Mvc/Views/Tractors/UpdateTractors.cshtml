    function LoadIconImage(ImgRowId, result, UpdId, FileExtensions) {

        //debugger;

        var input = $(result);
        if (result[0].value == '') {
            $(ImgRowId).hide();
        }
        else {

            var extension = $(result).val().split('.').pop().toLowerCase();
            var validFileExtensions = [FileExtensions];
            if (($.inArray(extension, validFileExtensions) == -1) && (FileExtensions != "")) {

               /// $(SpanId).text("Upload only " + validFileExtensions + " file").show();
                $(ImgRowId).hide();
                input.removeClass('is-valid');
                input.addClass('is-invalid');
                $(result).replaceWith($(result).val('').clone(true));
                result[0].value = '';
            }
            else {
                $(ImgRowId).show();
                $(UpdId).val(1);
                result.removeClass('is-invalid');
                result.addClass('is-valid');
                return true;
            }


        }

    }
